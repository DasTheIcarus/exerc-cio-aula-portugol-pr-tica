programa {
  funcao inicio() {
    cadeia cidadeMora, cidadeEstuda

    escreva("Qual o nome da cidade em que você mora?\n")
    leia(cidadeMora)
    escreva("Qual o nome da cidade em que você estuda?\n")
    leia(cidadeEstuda)
